@using System.Data

@model DataTable

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table>
        <tr>
            <th>
               FullName
            </th>
            <th>
              Job
            </th>
          
        </tr>
    
    @foreach (DataRow row in Model.Rows) {
        <tr>
            <td>
                @Html.Raw(row["FullName"])
            </td>
            <td>
                @Html.Raw(row["Job"])
            </td>
           
        </tr>
    }
    
    </table>
</body>
</html>
